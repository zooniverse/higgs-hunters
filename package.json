{
  "name": "lhc",
  "version": "0.0.1",
  "dependencies": {
    "publisssh": "^0.2.6",
    "zooniverse": "^0.7.1",
    "zooniverse-readymade": "^0.4.2"
  },
  "scripts": {
    "start": "zooniverse-readymade serve --port 3050 --project ./project.coffee --js overrides.coffee --css ./project.styl",
    "build": "zooniverse-readymade build --output ./build --project ./project.coffee --js overrides.coffee --css ./project.styl",
    "demo": "npm run build && publisssh ./build demo.zooniverse.org/lhc && rm -rf ./build",
    "beta": "npm run build && publisssh ./build zooniverse-static/www.higgshunters.org/beta && rm -rf ./build"
  }
}
